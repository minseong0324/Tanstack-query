{
  "name": "@tanstack/react-query-component-focus",
  "version": "5.74.0",
  "description": "Component-level focus management for TanStack Query in MFA environments",
  "author": "Tanner Linsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/query.git",
    "directory": "packages/react-query-component-focus"
  },
  "homepage": "https://tanstack.com/query",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "type": "module",
  "types": "build/lib/index.d.ts",
  "main": "build/lib/index.cjs",
  "module": "build/lib/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./build/lib/index.d.ts",
        "default": "./build/lib/index.js"
      },
      "require": {
        "types": "./build/lib/index.d.cts",
        "default": "./build/lib/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./build",
    "test:eslint": "eslint ./src",
    "test:types": "pnpm run \"/^test:types:ts[0-9]{2}$/\"",
    "test:types:ts50": "node ../../node_modules/typescript50/lib/tsc.js",
    "test:types:ts51": "node ../../node_modules/typescript51/lib/tsc.js",
    "test:types:ts52": "node ../../node_modules/typescript52/lib/tsc.js",
    "test:types:ts53": "node ../../node_modules/typescript53/lib/tsc.js",
    "test:types:ts54": "tsc",
    "test:lib": "vitest",
    "test:lib:dev": "pnpm run test:lib --watch",
    "test:build": "publint --strict",
    "build": "pnpm build:tsup && pnpm build:codemods",
    "build:tsup": "tsup",
    "build:codemods": "cpy ../query-codemods/src/**/* ./build/codemods"
  },
  "dependencies": {
    "@tanstack/query-core": "workspace:*"
  },
  "devDependencies": {
    "@tanstack/react-query": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "@tanstack/query-core": "workspace:*",
    "@tanstack/react-query": "workspace:*",
    "react": "^18 || ^19"
  }
}
